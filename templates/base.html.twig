    <script type="text/javascript">
        $(document).ready(function(){
            $("#colosio").click(function(){
                $('#map_canvas').show(); //muestro mediante id
               $('#map_canvas2').hide(); //muestro mediante clase
             });
            $("#huayacan").click(function(){
                $('#map_canvas').hide(); //oculto mediante id
                $('#map_canvas2').show(); //muestro mediante clase
            });
        });
    </script>
    <script>
      $(function(){
        $(".button").click(function(){
          var des=$(this).attr("id");
          $(".button").removeClass("tab-active");
          $('#resultado').val($(this).attr("data-rel")); 
          $("#"+des).addClass("tab-active");
        });
      });
    </script>
    <script>
  $(function() {
    $( ".datepicker1" ).datepicker({ minDate: 0, onClose: function( selectedDate ) {
        $( ".datepicker2" ).datepicker( "option", "minDate", selectedDate );
      } });
  
    $( ".datepicker2" ).datepicker({minDate: 0, onClose: function( selectedDate ) {
        $( ".datepicker1" ).datepicker( "option", "maxDate", selectedDate );}
      });
